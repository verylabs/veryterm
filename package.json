{
  "name": "veryterm",
  "version": "1.1.2",
  "description": "Multi-project terminal manager for vibe coding with Claude CLI",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck:renderer": "tsc --noEmit -p src/renderer/tsconfig.json",
    "typecheck:main": "tsc --noEmit -p tsconfig.node.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:renderer"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "electron-updater": "^6.7.3",
    "node-pty": "^1.0.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@tailwindcss/vite": "^4.0.6",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "postcss": "^8.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3"
  }
}
